# Environment configuration for CounselRank SEO Audit Service

# Symfony
APP_ENV=dev
APP_SECRET=!ChangeMe!
APP_DEBUG=true

# Database
DB_DSN="postgresql://audit_user:audit_password@postgres:5432/audit_service?serverVersion=16&charset=utf8"

# Redis
REDIS_DSN=redis://redis:6379
REDIS_CACHE_DSN=redis://redis:6379/1
REDIS_MESSENGER_DSN=redis://redis:6379/2

# Object Storage (S3-compatible)
OBJECT_STORE_ENDPOINT=http://minio:9000
OBJECT_STORE_BUCKET=audit-artifacts
OBJECT_STORE_ACCESS_KEY=minioadmin
OBJECT_STORE_SECRET_KEY=minioadmin
OBJECT_STORE_REGION=us-east-1
OBJECT_STORE_USE_PATH_STYLE=true

# Google APIs
GSC_CLIENT_ID=
GSC_CLIENT_SECRET=
GSC_REDIRECT_URI=http://localhost:8080/api/auth/google/callback

PSI_API_KEY=

# Audit Service Configuration
MAX_CONCURRENCY=10
HTTP_TIMEOUT_MS=30000
SCREENSHOTS_ENABLED=true
LIGHTHOUSE_SAMPLE_SIZE=30
MAX_PAGES_PER_AUDIT=10000

# Security
JWT_SECRET_KEY=!ChangeMe!
JWT_PASSPHRASE=!ChangeMe!
JWT_TOKEN_TTL=3600

# Monitoring
OPENTELEMETRY_ENDPOINT=http://jaeger:14268/api/traces
PROMETHEUS_METRICS_PORT=9090

# Development
CHROME_HEADLESS_URL=http://chrome:9222
